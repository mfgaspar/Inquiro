<?xml version="1.0" encoding="UTF-8"?>
<plugin title="Inquiro" loader="DEFAULT">
<overlays>
		<overlay id="startup.Inquiro" resourcebundle="content/Inquiro/resources/lang/messages">
			<menubar id="toolsmenu">
	        	<menuitem id="Inquiro" label="${Launcher.Inquiro}" command="mantleXulHandler.openUrl('${Launcher.Inquiro}','${Launcher.Inquiro_TOOLTIP}','plugin/Inquiro/api/default')"/>
	      	</menubar>
	    </overlay>
	</overlays>
	<lifecycle-listener class="pt.webdetails.cpk.CpkLifecycleListener"/>
	<static-paths>
		<static-path url="/Inquiro/static" localFolder="static"/>
		<static-path url="/Inquiro/resources" localFolder="resources"/>
	</static-paths>
	<content-types>
		<content-type>
			<title>Inquiro</title>
			<description></description>
			<company name="" url="" logo="@PLUGIN_COMPANY_LOGO@"></company>
		</content-type>
	</content-types>
	<content-generator
		id="Inquiro"
		title="Inquiro"
		type="Inquiro"
		class="pt.webdetails.cpk.CpkContentGenerator"/>
	<!-- Menu entry -->
	<menu-items>
		<!--<menu-item id="Inquiro_main" anchor="tools-submenu" label="Inquiro" command="content/Inquiro/" type="MENU_ITEM" how="LAST_CHILD"/>-->
	</menu-items>
</plugin>
